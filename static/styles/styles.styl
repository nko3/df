
.fp
  font-family Helvetica
  background #eee
  margin 40px 110px 30px 30px
  padding 30px 50px 120px
  
  h2
    font-size 40px
    margin 50px 0 30px
    
  ol 
    padding-bottom 40px
    li
      padding 8px 0
      font-weight bold
      font-size 18px
      &:first-child
        color #bbb
        text-decoration line-through
        -webkit-text-decoration-color #c00
        
  a
    color #196dc4
    text-decoration none
    &:hover
      text-decoration underline